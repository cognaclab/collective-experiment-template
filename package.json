{
  "name": "test-ex-gen-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node server/servers/www",
    "example": "concurrently \"npm run dev:web:example\" \"npm run dev:game:example\"",
    "experiment": "concurrently \"npm run dev:web:generated\" \"npm run dev:game:generated\"",
    "generate": "node scripts/generate-scenes.js",
    "generate:clean": "rm -rf client/public/src/generated/* client/views/generated/*",
    "generate:watch": "nodemon --watch content/experiments/ --ext md,yaml,yml --exec \"npm run generate\"",
    "dev:web:example": "EXPERIMENT_TYPE=example nodemon server/servers/www",
    "dev:web:generated": "EXPERIMENT_TYPE=generated nodemon --watch client/public/src/generated --watch client/views/generated server/servers/www",
    "dev:game": "nodemon server/servers/gameServer.js",
    "dev:game:example": "EXPERIMENT_TYPE=example nodemon server/servers/gameServer.js",
    "dev:game:generated": "EXPERIMENT_TYPE=generated nodemon server/servers/gameServer.js",
    "dev:full": "npm run docker:up && npm run experiment",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f mongodb",
    "docker:clean": "docker-compose down -v",
    "docker:shell": "docker exec -it collective-bandit-mongodb mongosh"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "bezier-easing": "^2.1.0",
    "browser-detect": "^0.2.28",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^17.2.1",
    "ejs": "^3.1.8",
    "express": "^4.21.2",
    "express-session": "^1.17.0",
    "fast-csv": "^4.3.6",
    "gray-matter": "^4.0.3",
    "http-errors": "~1.6.3",
    "jquery": "^3.5.1",
    "js-yaml": "^4.1.0",
    "marked": "^16.2.1",
    "mongoose": "^8.15.0",
    "morgan": "^1.10.1",
    "phaser": "^3.90.0",
    "socket.io": "4.8",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.0"
  }
}
